{
  "name": "github-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run-p dev:front dev:back",
    "dev:front": "vite --open",
    "dev:back": "json-server server.json -p 3333 --watch -d 500",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .  --ext .tsx,.ts",
    "lint:fix": "eslint .  --ext .tsx,.ts --fix"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "react-router-dom": "^6.9.0",
    "remark-gfm": "^3.0.1",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}